<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:ClashCs.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ClashCs.Views.SettingView"
             xmlns:fa="using:FluentAvalonia.UI.Controls"
             x:DataType="viewModels:SettingViewModel">

    <UserControl.DataContext>
        <viewModels:SettingViewModel />
    </UserControl.DataContext>

    <ScrollViewer>
        <StackPanel>
            <fa:SettingsExpander Header="sss" Description="开机启动" IsExpanded="False">
                <ToggleSwitch />
            </fa:SettingsExpander>
        </StackPanel>

    </ScrollViewer>

</UserControl>